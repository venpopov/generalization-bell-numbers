Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Define", " ", "the", " ", "recurrence", " ", "relation", " ", "for", " ", 
    "Z", 
    RowBox[{"(", 
     RowBox[{"n", ",", "k"}], ")"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Clear", "[", "Z", "]"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"Z", "[", 
      RowBox[{"0", ",", "0", ",", "m_"}], "]"}], "=", "1"}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"Z", "[", 
      RowBox[{"0", ",", "k_", ",", "m_"}], "]"}], ":=", 
     RowBox[{"0", "/;", 
      RowBox[{"k", ">", "0"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"Z", "[", 
      RowBox[{"n_", ",", "0", ",", "m_"}], "]"}], ":=", 
     RowBox[{"0", "/;", 
      RowBox[{"n", ">", "0"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"Z", "[", 
      RowBox[{"n_", ",", "k_", ",", "m_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"Z", "[", 
       RowBox[{"n", ",", "k", ",", "m"}], "]"}], "=", 
      RowBox[{
       RowBox[{"m", "*", 
        RowBox[{"Floor", "[", 
         RowBox[{"k", "/", "m"}], "]"}], "*", 
        RowBox[{"Z", "[", 
         RowBox[{
          RowBox[{"n", "-", "1"}], ",", "k", ",", "m"}], "]"}]}], "+", 
       RowBox[{"Z", "[", 
        RowBox[{
         RowBox[{"n", "-", "1"}], ",", 
         RowBox[{"k", "-", "1"}], ",", "m"}], "]"}]}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"marray", "[", 
      RowBox[{"m_", ",", " ", "nmax_"}], "]"}], " ", ":=", " ", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Z", "[", 
        RowBox[{"n", ",", "k", ",", "m"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "0", ",", "nmax"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "0", ",", "n"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rowSums", "[", 
     RowBox[{"m_", ",", " ", "nmax_"}], "]"}], " ", ":=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{"Z", "[", 
         RowBox[{"n", ",", "k", ",", "m"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"k", ",", "0", ",", "n"}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"n", ",", "0", ",", " ", "nmax"}], "}"}]}], "]"}]}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Define", " ", "the", " ", "general", " ", "triangular", " ", "array", " ",
      "table", " ", "with", " ", "headers", " ", "and", " ", "padding"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"marrayTable", "[", 
     RowBox[{"m_", ",", "nmax_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "array", ",", "rowLabels", ",", "colLabels", ",", "fullArray"}], "}"}],
       ",", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Generate", " ", "the", " ", "basic", " ", "array"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"array", "=", 
        RowBox[{"marray", "[", 
         RowBox[{"m", ",", " ", "nmax"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"rowLabels", "=", 
        RowBox[{"Table", "[", 
         RowBox[{"n", ",", 
          RowBox[{"{", 
           RowBox[{"n", ",", "0", ",", "nmax"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"colLabels", "=", 
        RowBox[{"Table", "[", 
         RowBox[{"k", ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", "nmax"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "Combine", " ", "everything", " ", "into", " ", "a", " ", "Grid"}], 
        "*)"}], "\[IndentingNewLine]", 
       RowBox[{"fullArray", "=", 
        RowBox[{"Prepend", "[", 
         RowBox[{
          RowBox[{"MapThread", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Prepend", "[", 
              RowBox[{"#1", ",", "#2"}], "]"}], "&"}], ",", 
            RowBox[{"{", 
             RowBox[{"array", ",", "rowLabels"}], "}"}]}], "]"}], ",", 
          RowBox[{"Prepend", "[", 
           RowBox[{
            RowBox[{"colLabels", "[", 
             RowBox[{"[", 
              RowBox[{"1", ";;", 
               RowBox[{"nmax", "+", "1"}]}], "]"}], "]"}], 
            ",", "\"\<n\\\\k\>\""}], "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Grid", "[", "\[IndentingNewLine]", 
        RowBox[{"fullArray", ",", "\[IndentingNewLine]", 
         RowBox[{"Alignment", "->", "Right"}], ",", "\[IndentingNewLine]", 
         RowBox[{"Frame", " ", "->", " ", "All"}], ",", " ", 
         "\[IndentingNewLine]", 
         RowBox[{"Background", " ", "->", " ", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"LightGray", ",", " ", "None"}], "}"}], ",", " ", 
            RowBox[{"{", 
             RowBox[{"LightGray", ",", " ", "None"}], "}"}]}], "}"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Spacings", "->", 
          RowBox[{"{", 
           RowBox[{"1", ",", "1"}], "}"}]}]}], "                 ", 
        "\[IndentingNewLine]", "]"}]}]}], "]"}]}], 
   "\[IndentingNewLine]"}]}]], "Input",
